
public class Admi {
private String name;
private String pw;
//获得和设置参数的方法
public Admi() {
	super();
}
public Admi(String name,String pw) {
	super();
	this.name=name;
	this.pw=pw;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name=name;
}
public String getPw() {
	return pw;
}
public void setPw(String pw) {
	this.pw=pw;
}
}
